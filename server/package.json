{
  "name": "express",
  "version": "1.0.0",
  "description": "Starter kit for creating Next and Express apps",
  "main": "server.js",
  "scripts": {
    "client-install": "cd ../client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run dev --prefix ../client",
    "dev": "concurrently \"npm run server\" \"cd ../client && npm run dev\""
  },
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2"
  }
}
